{
  "name": "procedure-recorder-desktop",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main/index.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "electron-rebuild -f -w robotjs",
    "dev": "vite",
    "build": "npm run build:preload && npm run build:electron && tsc && vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:preload": "tsc -p tsconfig.preload.json",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "dev:all": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run dev:electron\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "active-win": "^9.0.0",
    "autoprefixer": "^10.4.20",
    "electron": "^28.0.0",
    "electron-builder": "^25.1.8",
    "lucide-react": "^0.471.1",
    "postcss-loader": "^8.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17",
    "uiohook-napi": "^1.5.4",
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@electron/rebuild": "^3.7.1",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "typescript": "^5.5.3",
    "wait-on": "^8.0.2"
  },
  "packageManager": "pnpm@8.15.6+sha512.77b89e9be77a2b06ad8f403a19cae5e22976f61023f98ad323d5c30194958ebc02ee0a6ae5d13ee454f6134e4e8caf29a05f0b1a0e1d2b17bca6b6a1f1159f86"
}
